{
    "trafficLights": {
        "yesno": {
            "type": "key_value",
            "values": {
                "Yes": "green",
                "No": "red"
            }
        },
        "noyes": {
            "type": "key_value",
            "values": {
                "Yes": "red",
                "No": "green"
            }
        },
        "yesnona": {
            "type": "key_value",
            "values": {
                "Yes": "green",
                "No": "red",
                "N/A": "grey"
            }
        },
        "lowurgent": {
            "type": "key_value",
            "values": {
                "Low/OK": "green",
                "Caution": "yellow",
                "Warning": "orange",
                "Danger": "red",
                "Critical": "darkred",
                "Undefined": "grey"
            }
        },
        "niveaucongestionsite": {
            "type": "math",
            "values": {
                "x>30": "green",
                "x>15 and x<=30": "orange",
                "x<=15": "red"
            }
        },
        "nfidistrange": {
            "type": "key_value",
            "values": {
                "other": "red",
                "within the last 3 months": "orange",
                "within the last 2 months": "yellow",
                "within the last 1 month": "green"
            }
        },
        "TotalShowersVsIndidual": {
            "type": "math",
            "scope": {
                "x": "Nbdouche",
                "y": "demo_num_indv"
            },
            "values": {
                "(y/x)<= 20": "green",
                "(y/x)> 20 and (y/x)<=50 ": "orange",
                "(y/x)> 50 ": "red"
            }
        },
        "TotalLatrinesVsIndidual": {
            "type": "math",
            "scope": {
                "x": "NombreLatrines",
                "y": "demo_num_indv"
            },
            "values": {
                "(((y/x)*100)/20)< 100": "green",
                "(((y/x)*100)/20) >=100 and (((y/x)*100)/20)<200 ": "yellow",
                "(((y/x)*100)/20)>= 200 and (((y/x)*100)/20) <1000": "red",
                "(((y/x)*100)/20)>= 1000": "darkred"
            }
        },
        "TotalLatrines": {
            "type": "math",
            "scope": {
                "x": "NombreLatrines"
            },
            "values": {
                "x< 10": "darkred",
                "x>=10 and x<50 ": "red",
                "x>= 50 and x<100": "yellow",
                "x>= 500": "green"
            }
        },
        "ShelterCondition": {
            "type": "key_value",
            "values": {
                "I don’t know": "grey",
                "None/almost no shelters": "green",
                "Few shelters": "yellow",
                "Half of shelters": "orange",
                "Majority of shelters": "red",
                "All/almost all shelters": "darkred"
            }
        },
         "need_hh": {
            "type": "key_value",
            "values": {
                "all_almost_all_households": "green",
                "majority_of_households": "yellow",
                "half_of_households": "orange",
                "few_households": "red",
                "none_almost_no_households": "darkred"
            }
        },
         "female_sick": {
            "type": "key_value",
            "values": {
                "none": "green",
                "few": "yellow",
                "lot": "orange",
                "all": "red"
            }
        },
        "Sourceeau": {
            "type": "list_count",
            "values": {
                "x<1": "red",
                "x>=1 and x<4": "orange",
                "x>=4": "green"
            }
}

    }
}
